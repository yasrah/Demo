<div class="row" ng-controller="MyPageController" ng-cloak>
    {{ProductInventory}}
    <div class="col-lg-6">
        <div class="panel panel-default">
            <div class="panel-heading">
                Min side
            </div>
            <div class="panel-body">
                <div class="col-lg-12">
                    <div class="row">
                        <form role="form" id="contact" >
                            <fieldset disabled>
                                <div class="form-group">
                                    <label for="Id">Id</label>
                                    <input class="form-control" ng-model="myPageData.Id" id="Id" type="text" placeholder="Disabled input">
                                </div>
                                <div class="form-group">
                                    <label for="Name">Name</label>
                                    <input class="form-control" ng-model="myPageData.Name" id="Name" type="text" placeholder="Name">
                                </div>
                                <div class="form-group">
                                    <label for="Name">Login</label>
                                    <input class="form-control" ng-model="myPageData.Login" id="Login" type="text" placeholder="Name">
                                </div>
                                <div class="form-group">
                                    <label for="Name">MemberType</label>
                                    <input class="form-control" ng-model="myPageData.MemberType" id="MemberType" type="text" placeholder="Name">
                                </div>
                                <div class="form-group">
                                    <label for="Name">MemberGroup</label>
                                    <input class="form-control" ng-model="myPageData.MemberGroup" id="MemberType" type="text" placeholder="Name">
                                </div>
                                <div class="form-group">
                                    <label for="Name">FirstName</label>
                                    <input class="form-control" ng-model="myPageData.FirstName" id="FirstName" type="text" placeholder="Name">
                                </div>
                                <div class="form-group">
                                    <label for="Name">LastName</label>
                                    <input class="form-control" ng-model="myPageData.LastName" id="LastName" type="text" placeholder="Name">
                                </div>
                                <a href="/editmypage" class="">Endre</a>
                            </fieldset>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="col-lg-6">
        <div class="panel panel-default">
            <div class="panel-heading">
                Lagerstatus
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <div class="table-responsive">
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>Product</th>
                                <th>Lagerstatus</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="pi in myPageData.ProductInventory">
                                <td>{{$index+1}}</td>
                                <td>{{pi.ProductId}}</td>
                                <td>{{pi.Amount}}</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <!-- /.table-responsive -->
            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
</div>
